{
  "name": "gajevids-schema",
  "displayName": "Gajevids Schema",
  "description": "Provides YAML schema validation and autocompletion for gajevids.yml files.",
  "version": "0.0.2",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "contributes": {
    "yaml.schemas": [
      {
        "fileMatch": [
          "gajevids.yml",
          "*vids.yml"
        ],
        "url": "./schemas/gajevids.schema.json"
      }
    ]
  },
  "extensionDependencies": [
    "redhat.vscode-yaml"
  ]
}

{
  "name": "gajevids-schema",
  "displayName": "Gajevids Schema",
  "description": "Provides YAML schema validation and autocompletion for gajevids.yml files.",
  "version": "0.0.5",
  "publisher": "YOUR-PUBLISHER-ID",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Snippets",
    "Programming Languages"
  ],
  "extensionDependencies": [
    "redhat.vscode-yaml"
  ],
  "contributes": {
    "yamlValidation": [
      {
        "fileMatch": [
          "gajevids.yml",
          "gajevids.yaml",
          "*.gajevids.yml",
          "*.gajevids.yaml"
        ],
        "url": "./gajevids.schema.json"
      }
    ]
  }
}
